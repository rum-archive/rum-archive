{
  "name": "rum-archive",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "Akamai Technologies, Inc",
  "license": "MIT",
  "scripts": {
    "build": "npm run css-deploy && npm run js-build && npm run eleventy-build",
    "start": "npm-run-all --parallel css-watch js-watch eleventy-watch",
    "css-build": "sass src/_sass/main.scss src/css/main.css",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-postcss": "node_modules/.bin/postcss --map --use autoprefixer --output src/css/main.css src/css/main.css",
    "css-watch": "npm run css-build -- --watch",
    "js-build": "babel src/_javascript --out-dir src/js",
    "js-watch": "npm run js-build -- --watch",
    "eleventy-watch": "eleventy --serve",
    "eleventy-build": "eleventy"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
    "autoprefixer": "^10.4.12",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-ie": "^6.7.0",
    "bulma": "^0.9.4",
    "luxon": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "postcss-cli": "^10.0.0",
    "prismjs": "^1.29.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0"
  },
  "dependencies": {
    "eleventy-plugin-toc": "^1.1.5",
    "highcharts": "^10.3.2",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5"
  }
}
